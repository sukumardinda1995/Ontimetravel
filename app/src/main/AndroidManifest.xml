<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.ontimetravel.ontimetravel">

    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Feedback" />
        <activity
            android:name=".SearchData"
            android:label="@string/list"
            android:theme="@style/AppThemelist" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppThemeMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VerifyPhoneActivity"
            android:theme="@style/AppThemeVerifyPhone" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/profilemain"
            android:theme="@style/AppThemeProfileActivity" />
        <activity android:name=".SearchData1" />

    </application>

</manifest>